/config/rp_config_twitter_ngzm.yml
/config/environments/smtp_settings_ngzm.yml

# public
/public/uploads

# Client
/public/js/build.js
/client/node_modules
/client/dist/index.html
/client/dist/build.js*
/client/npm-debug.log

# Staging
/staging

