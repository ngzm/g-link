<section id="admin">
  <%= link_to 'Main', admin_admin_main_path %> |
  <%= link_to 'Users', admin_users_path %> |
  <hr />

  <p id="notice"><%= notice %></p>

  <div class="showdata">
    <table>
      <tr><th>ID</th><td><%= @user.id %></td></tr>
      <tr><th>Name</th><td><%= @user.name %></td></tr>
      <tr><th>Full Name</th><td><%= @user.full_name %></td></tr>
      <tr><th>Email</th><td><%= @user.email %></td></tr>
      <tr><th>Picture</th><td><%= @user.picture %></td></tr>
      <tr><th>IMG</th><td><%= image_tag @user.picture.html_safe, size: '96x96' unless @user.picture.nil? %></td></tr>
      <tr><th>Admin</th><td><%= @user.admin %></td></tr>
      <tr><th>Provider</th><td><%= @user.provider %></td></tr>
      <tr><th>Identifier</th><td><%= @user.identifer %></td></tr>
      <tr><th>CREATE</th><td><%= @user.created_at %></td></tr>
      <tr><th>UPDATE</th><td><%= @user.updated_at %></td></tr>
    </table>
  </div>
</section>
